<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jonda.team.repository.ActivityRepository">
    <resultMap id="activityResultMap" type="com.jonda.team.repository.entity.ActivityEntity">
        <result column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="team_name" property="teamName"/>
        <result column="master_id" property="masterId"/>
        <result column="min_score" property="minScore"/>
        <result column="start_time" property="start" jdbcType="TIMESTAMP" javaType="java.util.Date"/>
        <result column="end_time" property="end" jdbcType="TIMESTAMP" javaType="java.util.Date"/>
        <result column="remark" property="remark"/>
    </resultMap>
</mapper>